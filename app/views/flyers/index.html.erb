<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flyer.each do |flyer| %>
      <tr>
        <td><%= flyer.title %></td>
        <td><%= flyer.address %></td>
        <td><%= link_to 'Show', flyer %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flyers', new_flyer_path %>
